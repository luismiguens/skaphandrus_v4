{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SkPhotoContest</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ entity.logo }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ entity.image }}</td>
            </tr>
            <tr>
                <th>Beginat</th>
                <td>{{ entity.beginAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Endat</th>
                <td>{{ entity.endAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Isjudge</th>
                <td>{{ entity.isJudge }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('photo_contest_admin') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('photo_contest_admin_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
