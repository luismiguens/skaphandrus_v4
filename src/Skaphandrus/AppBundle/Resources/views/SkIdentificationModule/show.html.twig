{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SkIdentificationModule</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Appstoreid</th>
                <td>{{ entity.appstoreId }}</td>
            </tr>
            <tr>
                <th>Googleplayid</th>
                <td>{{ entity.googleplayId }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ entity.isActive }}</td>
            </tr>
            <tr>
                <th>Isenabled</th>
                <td>{{ entity.isEnabled }}</td>
            </tr>
            <tr>
                <th>Isfree</th>
                <td>{{ entity.isFree }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ entity.image }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('identification_module_admin') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('identification_module_admin_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
