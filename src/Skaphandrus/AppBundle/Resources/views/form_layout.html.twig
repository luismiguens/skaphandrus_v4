<!DOCTYPE html>
<html>

    <head>

        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <title>INSPINIA | Minor view</title>

        <link href="{{ asset('bundles/skaphandrusapp/css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/skaphandrusapp/font-awesome/css/font-awesome.css') }}" rel="stylesheet">

        <link href="{{ asset('bundles/skaphandrusapp/css/animate.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/skaphandrusapp/css/style.css') }}" rel="stylesheet">
        <link href="http://code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.css" rel="Stylesheet"></link>

        {% block stylesheets %}{% endblock %}
    </head>





    <body class="boxed-layout">
        <div id="wrapper">
            {% include 'SkaphandrusAppBundle:Default:sidebar-menu.html.twig' with {'layout': 'form'} %}
            <div id="page-wrapper" class="gray-bg">
                <div class="row border-bottom">
                    {% include 'SkaphandrusAppBundle:Default:navbar-menu.html.twig' with {'bg': 'white-bg'} %}
                </div>
                <!-- page-heading begin -->
                <div class="row wrapper border-bottom white-bg page-heading">
                    <div class="col-sm-4">
                        <h2>{% block form_title %}{% endblock %}</h2>
                    {% block form_breadcrumbs %}{% endblock %}
                </div>
                <div class="col-sm-8">
                    <div class="title-action">
                        <a href="" class="btn btn-primary">{% block form_title_action %}{% endblock %}</a>
                    </div>
                </div>
            </div>
            <!-- page-heading end -->
            <div class="wrapper wrapper-content animated fadeInRight">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>{% block form_title_description %}{% endblock %}</h5>
                            </div>
                            <div class="ibox-content">
                                {% block body %}
                                {% endblock %}
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="footer">
                <div class="pull-right">
                    10GB of <strong>250GB</strong> Free.
                </div>
                <div>
                    <strong>Copyright</strong> Example Company &copy; 2014-2015
                </div>
            </div>

        </div>
    </div>



    {% block javascripts %}
        <!-- Mainly scripts -->
        <script src="{{ asset('bundles/skaphandrusapp/js/jquery-2.1.1.js') }}"></script>
        <script src="{{ asset('bundles/skaphandrusapp/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/skaphandrusapp/js/plugins/metisMenu/jquery.metisMenu.js') }}"></script>
        <script src="{{ asset('bundles/skaphandrusapp/js/plugins/slimscroll/jquery.slimscroll.min.js') }}"></script>

        <!-- Custom and plugin javascript -->
        <script src="{{ asset('bundles/skaphandrusapp/js/inspinia.js') }}"></script>
        <script src="{{ asset('bundles/skaphandrusapp/js/plugins/pace/pace.min.js') }}"></script>

    {% endblock %}
</body>

</html>






