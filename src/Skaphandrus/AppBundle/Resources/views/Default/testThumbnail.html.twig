{% extends 'SkaphandrusAppBundle::content_index_layout.html.twig' %}

{% block body %}

     {% for photo in photos %}
        <a href="{{ url_to_photo(photo) }}" title="{{ photo.title }}" >
            <img alt="{{ photo.title }}" src="{{ photo.getWebPath | imagine_filter('sk_widen_590') }}" />
        </a>
    {% endfor %}

{% endblock %}

