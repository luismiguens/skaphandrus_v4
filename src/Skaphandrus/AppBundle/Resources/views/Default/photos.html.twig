            <li itemprop="itemListElement"><a href="{{ url_to_photos( {'country': pagination[0].spot.location.region.country.id}) }}">{{pagination[0].spot.location.region.country}}</a></li>
            <li itemprop="itemListElement"><a href="{{ url_to_photos( {'region': pagination[0].spot.location.region.id}) }}">{{pagination[0].spot.location.region.name}}</a></li>
            <li itemprop="itemListElement" class="active"><strong>{{ pagination[0].spot.location.name}} </strong></li>

        {% elseif params.region is defined %}

            <li itemprop="itemListElement"><a href="{{ url_to_photos( {'country': pagination[0].spot.location.region.country.id}) }}">{{pagination[0].spot.location.region.country}}</a></li>
            <li itemprop="itemListElement" class="active"><strong>{{ pagination[0].spot.location.region.name}} </strong></li>

        {% elseif params.country is defined %}
            <li itemprop="itemListElement" class="active"><strong>{{ pagination[0].spot.location.region.country}} </strong></li>

            <!-- fosUser -->
        {% elseif params.fosUser is defined %}

            <li itemprop="itemListElement" class="active"><strong>{{ pagination[0].fosUser}} </strong></li>

        {% else %}
            <li itemprop="itemListElement" class="active"><strong>{{ "breadcrumb.photos"|trans }}  </strong></li>
            {% endif %}
    </ol>
{% endblock %}

