{% block body %}
    <div class="wrapper wrapper-content" itemscope itemtype="http://schema.org/ImageGallery">
        {#-----h1 title-----#}
        {% if params.species is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.species"|trans, '%value%': pagination[0].species.name} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.genus is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.genus"|trans, '%value%': pagination[0].species.genus.name} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.family is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.family"|trans, '%value%': pagination[0].species.genus.family.name} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.order is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.order"|trans, '%value%': pagination[0].species.genus.family.order.name} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.class is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.class"|trans, '%value%': pagination[0].species.genus.family.order.class.name} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.phylum is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.phylum"|trans, '%value%': pagination[0].species.genus.family.order.class.phylum.name} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.kingdom is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.kingdom"|trans, '%value%': pagination[0].species.genus.family.order.class.phylum.kingdom.name} %} page.photos.h1.title {% endtrans %}</h1>

            {#-----spots-----#}
        {% elseif params.spot is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.spot"|trans, '%value%': pagination[0].spot.name~", "~pagination[0].spot.location.name~", "~pagination[0].spot.location.region.country} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.location is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.location"|trans, '%value%': pagination[0].spot.location.name~", "~pagination[0].spot.location.region.name~", "~pagination[0].spot.location.region.country} %} page.photos.h1.title {% endtrans %}</h1>
        {% elseif params.region is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.region"|trans, '%value%': pagination[0].spot.location.region.name~", "~pagination[0].spot.location.region.country} %} page.photos.h1.title {% endtrans %}</h1>  
        {% elseif params.country is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.country"|trans, '%value%': pagination[0].spot.location.region.country} %} page.photos.h1.title {% endtrans %}</h1>

            {#-----fosuser-----#}
        {% elseif params.fosUser is defined %}
            <h1>{% trans with {'%type%': "page.photos.keyword.fosUser"|trans, '%value%': pagination[0].fosUser} %} page.photos.h1.title {% endtrans %}</h1>

            {#-----someting else-----#}
        {% else %}
            <h1>{{ "page.photos_something_else.h1.title"|trans }}</h1>
        {% endif %}

        <br>

        <div class="row gallery">
            {% for photo in pagination %}
                <div class="col-lg-3 col-md-6 col-sm-12 grid-item-gallery">
                    <div class="ibox float-e-margins">
                        <!--<div class="ibox-title"><h5>{{photo.title}}</h5></div>-->
                        <div itemprop="image" itemscope itemtype="http://schema.org/ImageObject">
                            <div class="ibox-content ibox-content no-padding border-left-right">
                                <meta itemprop="name" content="{{photo.title}}">
                                <a href="{{ url_to_photo(photo) }}" title="{{ photo.title }}">
                                    <img itemprop="image" alt="{{photo.title}}" class="img-responsive" src="{{ photo.getWebPath | imagine_filter('sk_widen_400') }}" />
                                </a>
                            </div>
                            <div class="ibox-content profile-content">
                                <h4><strong>{{ photo.title }}</strong></h4>
                                <ul class="list-unstyled">
                                    <li itemprop="author" itemscope itemtype="http://schema.org/Person">
                                        <meta itemprop="name" content="{{ photo.fosUser.name }}">
                                        {{ "page.photos.label.username"|trans }}: <a href="{{ url_to_user(photo.fosUser) }}">{{ photo.fosUser.name }}</a>
                                    </li>
                                    {% if photo.spot is not null %}
                                        <li itemprop="contentLocation" itemscope itemtype="http://schema.org/TouristAttraction">
                                            <meta itemprop="name" content="{{ photo.spot.name }}">
                                            {{ "page.photos.label.spot"|trans }}: <a href="{{ url_to_spot(photo.spot) }}">{{ photo.spot.name }}</a>
                                        </li>
                                    {% endif %}
                                    {% if photo.species is not null %}
                                        <li>
                                            {{ "page.photos.label.species"|trans }}: <a href="{{ url_to_species(photo.species) }}">{{ photo.species.name }}</a>
                                        </li>
                                    {% endif %}
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

            {% endfor %}

        </div>
        <div class="navigation">
            {{ knp_pagination_render(pagination) }}
        </div>

    </div>
{% endblock %}
