<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Skaphandrus\AppBundle\Entity\SkUserPoints" repository-class="Skaphandrus\AppBundle\Entity\Repository\SkUserPointsRepository" table="sk_user_points">
    <indexes>
      <index name="sk_user_points_FI_1" columns="points_type_id"/>
      <index name="sk_user_points_FK_2" columns="fos_user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="points" type="integer" column="points" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
   
    <many-to-one field="fosUser" target-entity="FosUser" fetch="LAZY">
      <join-columns>
        <join-column name="fos_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pointsType" target-entity="SkPointsType" fetch="LAZY">
      <join-columns>
        <join-column name="points_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
