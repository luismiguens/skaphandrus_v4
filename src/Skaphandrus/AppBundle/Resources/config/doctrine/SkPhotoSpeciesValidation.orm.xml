<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Skaphandrus\AppBundle\Entity\SkPhotoSpeciesValidation" table="sk_photo_species_validation">
    <indexes>
      <index name="sk_photo_species_validation_FI_1" columns="photo_id"/>
      <index name="sk_photo_species_validation_FI_2" columns="species_id"/>
      <index name="sk_photo_species_validation_FI_3" columns="fos_user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rating" type="integer" column="rating" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <many-to-one field="fosUser" target-entity="FosUser" fetch="LAZY">
      <join-columns>
        <join-column name="fos_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="species" target-entity="SkSpecies" fetch="LAZY">
      <join-columns>
        <join-column name="species_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="photo" target-entity="SkPhoto" fetch="LAZY">
      <join-columns>
        <join-column name="photo_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
