<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Skaphandrus\AppBundle\Entity\SkTermsConditionsTranslation" table="sk_terms_conditions_translation">
        <indexes>
            <index name="fk_sk_terms_conditions_translation_1_idx" columns="translatable_id"/>
        </indexes>
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="locale" type="string" column="locale" length="45" nullable="false">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="text" type="text" column="text" length="2000" nullable="true">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <many-to-one field="translatable" target-entity="SkTermsConditions" fetch="LAZY">
            <join-columns>
                <join-column name="translatable_id" referenced-column-name="id"/>
            </join-columns>
        </many-to-one>
    </entity>
</doctrine-mapping>
