<?xml version="1.0" encoding="utf-8"?>

<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Skaphandrus\AppBundle\Entity\SkUserResults" table="sk_user_results">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="name" type="string" column="name" length="255">
            <options>
                <option name="fixed"/>
            </options>
        </field>
        <field name="photos" type="integer" column="photos" />
        <field name="species" type="integer" column="species" />
        <field name="validations" type="integer" column="validations" />
        <field name="sugestions" type="integer" column="sugestions" />
        <field name="points" type="integer" column="points" />
    </entity>
    
</doctrine-mapping>
