<?php

namespace Skaphandrus\AppBundle\Entity;

/**
 * SkCreativeCommons
 */
class SkCreativeCommons
{
    /**
     * @var string
     */
    private $image;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set image
     *
     * @param string $image
     *
     * @return SkCreativeCommons
     */
    public function setImage($image)
    {
        $this->image = $image;

        return $this;
    }

    /**
     * Get image
     *
     * @return string
     */
    public function getImage()
    {
        return $this->image;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }
}
